    "                if j%n>0:\n",
    "                    changes.append((i+1)*n+j-1)\n",
    "                if j%n<n-1:\n",
    "                    changes.append((i+1)*n+j+1)\n",
    "                if i<n-2:\n",
    "                    changes.append((i+2)*n+j)\n",
    "                #print(changes)\n",
    "                for k in changes:\n",
    "                    solver[k]=(solver[k]+1)%2\n",
    "                #print(solver)\n",
